{
    "name": "kaadon/kaadonbaseadmin",
    "description": "基于ThinkPHP6.0和layui的快速开发的后台管理系统。",
    "type": "project",
    "keywords": [
        "easyadmin",
        "thinkphp",
        "admin"
    ],
    "homepage": "http://easyadmin.99php.cn/",
    "license": "MIT",
    "authors": [
        {
            "name": "kaadon",
            "email": "kaadon@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
      "topthink/framework": "^6.0",
      "topthink/think-orm": "^2.0",
      "topthink/think-multi-app": "^1.0",
      "topthink/think-view": "^1.0",
      "topthink/think-captcha": "^3.0",
      "aliyuncs/oss-sdk-php": "^2.4",
      "qcloud/cos-sdk-v5": "^2.0",
      "qiniu/php-sdk": "^7.4",
      "jianyan74/php-excel": "^1.0",
      "zhongshaofa/thinkphp-log-trace": "^1.0",
      "kaadon/kaadon-admin": "1.0.6",
      "topthink/think-worker": "^3.0",
      "kaadon/jwt": "^1.2",
      "kaadon/uuid": "^1.0",
      "kaadon/capcha": "^1.0",
      "topthink/think-queue": "3.0.7",
      "alibabacloud/client": "^1.5",
      "usdtcloud/tronservice":"^1.0",
      "ext-curl": "*",
      "ext-json": "*",
        "ipinfo/ipinfo": "^2.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2",
        "eaglewu/swoole-ide-helper": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app",
          "Kaadon\\Lock\\": "extend/kaadon/lock/src"
        }
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false
    },
    "scripts": {
        "post-autoload-dump": [
            "@php think service:discover",
            "@php think vendor:publish"
        ]
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.org"
        }
    }
}
